<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Onward.CreateUser"
             Title="CreateUser">
    <VerticalStackLayout>
        <Entry x:Name="Username" Placeholder="Username" />
        <Entry x:Name="Email" Placeholder="Email" />
        <Entry x:Name="Password" Placeholder="Password" />
        <Entry x:Name="Roles" Placeholder="Roles" />
        <Button Text="Cancel" Clicked="Cancel" />
        <Button Text="Submit" Clicked="Submit" />
    </VerticalStackLayout>
</ContentPage>